using OMetaSharp;
using YaYAML;

ometa YaYAML.Parsing.YamlParser : Parser {
  Sequence = SequenceItem+:items -> { items },
  SequenceItem = '-' Space Text:text (NewLine | Empty) -> { new YamlSequenceItem(text.As<string>()) },
  Text = (~NewLine Character)+:t -> { t.As<string>() },
  NewLine = '\r' '\n'
          | '\n'
}